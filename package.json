    "raf": "^3.3.0",
    "react": "^15.2.0",
    "react-addons-shallow-compare": "^15.2.0",
    "react-css-themr": "^1.2.0",
    "react-dom": "^15.2.0",
    "react-motion": "^0.4.4",
    "react-router": "^2.7.0",
    "react-virtualized": "^7.19.4",
    "recompose": "^0.20.2",
