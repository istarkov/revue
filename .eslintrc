
{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "mocha": true,
    "node": true
  },
  "rules": {
    "no-nested-ternary": 0,
    "react/prop-types": 0,
    "no-sequences": 1,
    "no-duplicate-imports": 0,
  },
  "globals": {
    "ReactElement": false,
    "$Shape": false
  }
}
